{
	"name": "popgenesjs",
	"private": true,
	"version": "1.0.0",
	"description": "Population Genetics Simulation Suite - Web Edition",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "npm run build && npx wrangler pages deploy build --project-name=popgenejs"
	},
	"keywords": [
		"population-genetics",
		"simulation",
		"biology"
	],
	"license": "MIT",
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/d3": "^7.4.3",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"wrangler": "^4.64.0"
	},
	"dependencies": {
		"d3": "^7.9.0"
	}
}
